{!REQUIRESCRIPT(&quot;/soap/ajax/48.0/apex.js&quot;)}

function invokeApexMethod() {
    // Get the record ID of the current record
    var recordId = &apos;{!Change_Request__c.Id}&apos;;
    
    // Invoke the Apex method
    sforce.apex.execute(&quot;servicenow&quot;, &quot;updatecr&quot;, { recordId: recordId }, function(result) {
